   "source": [
    "n = int(input(\"Ingrese un numero entero\"))\n",
    "s = 0\n",
    "i = 1\n",
    "while i <= n:\n",
    "    s = s + i\n",
    "    i +=1\n",
    "print(n,s, i, sep = \"-\")"
